    "def remove_extreme(points):\n",
    "    tmp = points[points[:,0].argsort()]\n",
    "    tmp = tmp[10:-10, :]\n",
    "    \n",
    "    tmp = tmp[tmp[:,1].argsort()]\n",
    "    tmp = tmp[10:-10, :]\n",
    "    return tmp"
